    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/friendly-service/src/app.js",
            "outFiles": [
                "${workspaceFolder}/friendly-service/src/app.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "preLaunchTask": "nb-tsc-watch",
            "timeout": 10000,
            "program": "${workspaceFolder}/src/app.ts",
            "restart": true,
            "cwd": "${workspaceRoot}",
            "outFiles": [
                "${workspaceFolder}/bin/**/*.js"
            ],
            "sourceMaps": true
        }
    ]
}